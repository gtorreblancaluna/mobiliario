	<queryString>
		<![CDATA[SELECT renta.descripcion AS descripcion_evento,renta.fecha_entrega,renta.fecha_evento,renta.fecha_devolucion,cliente.nombre AS nombre_cliente,
cliente.apellidos AS apellidos_cliente,cliente.tel_movil,cliente.tel_fijo, renta.id_renta, renta.folio, articulo.id_categoria, categoria.descripcion AS descripcion_categoria, articulo.id_articulo, articulo.descripcion AS descripcion_articulo,
color.color AS descripcion_color, detalle.cantidad AS cantidad_articulos, articulo.codigo AS codigo_articulo, usuarios.id_usuarios, usuarios.nombre AS nombre_usuario, usuarios.apellidos AS apellidos_usuario
FROM renta renta
INNER JOIN detalle_renta detalle ON (detalle.id_renta = renta.id_renta)
INNER JOIN articulo articulo ON (articulo.id_articulo = detalle.id_articulo)
INNER JOIN categoria categoria ON (categoria.id_categoria = articulo.id_categoria)
INNER JOIN color color ON (color.id_color = articulo.id_color)
INNER JOIN clientes cliente ON (cliente.id_clientes = renta.id_clientes)
INNER JOIN usuarios usuarios ON (usuarios.id_usuarios = renta.id_usuarios)
WHERE renta.id_renta = $P{ID_RENTA} AND articulo.id_categoria IN (SELECT asigna.id_categoria FROM asigna_categoria asigna WHERE asigna.id_usuarios = $P{ID_USUARIO})]]>
	</queryString>
