	<parameter name="IDS" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
            r.folio,
            r.fecha_evento,
            r.fecha_entrega,
            r.fecha_devolucion,
            r.hora_entrega,
            r.descripcion,
            CONCAT(c.nombre," ",c.apellidos) AS nombre_cliente,
            tipo_evento.tipo AS tipo_evento
        FROM
            renta r
            INNER JOIN usuarios u ON r.id_usuarios = u.id_usuarios
            INNER JOIN clientes c ON r.id_clientes = c.id_clientes
            INNER JOIN tipo tipo_evento ON (r.id_tipo = tipo_evento.id_tipo)
        WHERE
            r.id_renta IN ($P!{IDS})
            ORDER BY STR_TO_DATE(r.fecha_entrega, '%d/%m/%Y')]]>
	</queryString>
