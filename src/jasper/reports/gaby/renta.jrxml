	<queryString>
		<![CDATA[SELECT IF(d.porcentaje_descuento > 0 , (d.cantidad * d.p_unitario) * (d.porcentaje_descuento / 100) , 0 ) AS totalDescuento,
     CONCAT(u.`nombre`," ",u.`apellidos`) AS Usuario,
     IF(d.porcentaje_descuento > 0 , (d.cantidad * d.p_unitario) - ((d.cantidad * d.p_unitario) * (d.porcentaje_descuento / 100)  ) , (d.cantidad * d.p_unitario) ) AS Importe,
     CONCAT(a.`descripcion`," ",co.`color`) AS Articulo,
     CONCAT(c.`nombre`," ",c.`apellidos`) AS Cliente,
     r.`fecha_pedido`,
     r.`fecha_entrega`,
     r.`hora_entrega`,
     r.`fecha_devolucion`,
     r.`descripcion`,
     d.`cantidad` ,
     d.`p_unitario`,
     c.`direccion`,
     r.`comentario`,
     r.`folio`,
     ti.`tipo`,
     r.hora_devolucion,
     c.tel_movil,
     c.tel_fijo,
     r.fecha_evento,
r.envio_recoleccion,
r.deposito_garantia,
r.mostrar_precios_pdf

FROM
     `renta` r INNER JOIN `usuarios` u ON r.`id_usuarios` = u.`id_usuarios`
     INNER JOIN `detalle_renta` d ON r.`id_renta` = d.`id_renta`
     INNER JOIN `clientes` c ON r.`id_clientes` = c.`id_clientes`
     INNER JOIN `articulo` a ON d.`id_articulo` = a.`id_articulo`
     INNER JOIN `color` co ON a.`id_color` = co.`id_color`
     INNER JOIN `tipo` ti ON r.`id_tipo` = ti.`id_tipo`
WHERE
     r.`id_renta` = $P{id_renta}]]>
	</queryString>
