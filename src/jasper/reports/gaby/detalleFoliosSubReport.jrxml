	<parameter name="ID_RENTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
             d.cantidad,
             CONCAT(a.descripcion," ",color.color) AS articulo,
             categoria.descripcion AS categoria
        FROM detalle_renta d
             INNER JOIN articulo a ON d.id_articulo = a.id_articulo
             INNER JOIN color color ON a.id_color = color.id_color
             INNER JOIN categoria categoria ON a.id_categoria = categoria.id_categoria
        WHERE
             d.id_renta IN ($P!{ID_RENTA})
        ORDER BY a.descripcion]]>
	</queryString>
