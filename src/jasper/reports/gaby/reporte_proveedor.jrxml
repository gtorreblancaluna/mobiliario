	<queryString>
		<![CDATA[SELECT o.id AS orden,
r.fecha_entrega AS fecha_entrega,
r.fecha_evento AS fecha_evento,
r.fecha_devolucion AS fecha_devolucion,
r.folio AS folio,
a.codigo AS codigo_articulo,
d.cantidad AS cantidad,
d.comentario AS comentario_detalle,
o.comentario AS comentario_general,
CONCAT(a.descripcion," ",c.color) AS descripcion_articulo,
d.precio AS precio,
(d.cantidad * d.precio) AS total,
CONCAT(p.nombre," ",p.apellidos)AS nombre_proveedor
FROM detalle_orden_proveedor d
INNER JOIN orden_proveedor o ON (o.id = d.id_orden_proveedor)
INNER JOIN renta r ON (r.id_renta = o.id_renta)
INNER JOIN articulo a ON (a.id_articulo = d.id_articulo)
INNER JOIN color c ON (c.id_color = a.id_color)
INNER JOIN proveedores p ON(p.id = o.id_proveedores)
WHERE d.fg_activo = '1'
AND d.id_orden_proveedor = $P{ID_ORDEN}]]>
	</queryString>
